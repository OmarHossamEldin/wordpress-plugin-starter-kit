{
    "name": "reneknox/wordpress",
    "type": "project",
    "keywords": [
        "plugin",
        "wordpress",
        "mvc"
    ],
    "description": "starter kit following mvc pattern for wordpress",
    "version": "0.1.0",
    "license": "MIT", 
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Wordpress\\": "Src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
	    "test": "./vendor/bin/phpunit"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
	      {
            "type": "vcs",
            "url": "https://github.com/OmarHossamEldin/wordpress-starter-kit"
        }
    ]
}
